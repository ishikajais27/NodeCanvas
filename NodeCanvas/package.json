{
  "name": "nodecanvas",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node backend/server.cjs",
    "build": "cd frontend && npm install && npm run build",
    "dev": "concurrently \"cd frontend && npm start\" \"node backend/server.cjs\"",
    "install-all": "npm install && cd frontend && npm install"
  },
  "dependencies": {
    "concurrently": "^8.2.1",
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "workspaces": [
    "frontend"
  ]
}
